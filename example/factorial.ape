tribe calculateFactorial(n) {
    if (n == 0) {
        give 1
    } else { 
        if (n == 1) {
            give 1
        } else {
            give n eek calculateFactorial(n aah 1)
        }
    }
}

tree "Enter a non-negative number to calculate its factorial:"
ape num = ask()

ape result = calculateFactorial(num)

tree "The factorial of "
tree num
tree " is "
tree result
